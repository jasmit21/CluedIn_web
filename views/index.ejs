                    function ScheduleDate(){
                        let todayDate = new Date();
                            let month = todayDate.getMonth() + 1;
                            let year = todayDate.getUTCFullYear();
                            let tdate = todayDate.getDate();
                            if(tdate<10){
                                tdate = "0"+tdate;
                            }
                            if(month<10){
                                month = "0"+month;
                            }
                            let minDate = year+"-"+month+"-"+tdate;
                            return minDate;
                    }
                    function notifEventScheduleDateHandler(){
                        let notifMsgScheduleDate = document.getElementById("notifMsgScheduleDate").value;
                        let notifMsgExpiryDate = document.getElementById("notifMsgExpiryDate").value;
                        document.getElementById("notifEventdate").setAttribute("min",notifMsgScheduleDate);
                        document.getElementById("notifEventdate").setAttribute("max",notifMsgExpiryDate);
                        }
                    function notifLabelHandler(){
                        let notifEventSchedule = document.getElementById("notifEventSchedule");
                        let notifLabel = document.getElementById("notifLabel");
                        
                        if (notifLabel.value=="Event"){
                            notifEventSchedule.style.visibility="visible";
                            var minDate = ScheduleDate();
                            notifEventSchedule.setAttribute("min",minDate);
                            notifEventSchedule.setAttribute("max",document.getElementById("notifMsgExpiryDate").value);
                        }else{
                            notifEventSchedule.style.visibility="hidden";
                        }
                    }    
                    function notifMsgScheduleHandler(){
                        let minDate = ScheduleDate();
                        document.getElementById("notifMsgScheduleDate").setAttribute("min",minDate);
                    }
                    function notifMsgExpiryHandler(){
                        let notifMsgScheduleDate = document.getElementById("notifMsgScheduleDate").value;
                        document.getElementById("notifMsgExpiryDate").setAttribute("min",notifMsgScheduleDate);
                    }
